{
  "name": "sooq-mazad-monorepo",
  "version": "2.0.0",
  "description": "سوق مزاد - منصة مزادات السيارات الليبية الموحدة",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "packageManager": "npm@10.2.4",
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "cd apps/web && npm run dev",
    "dev:admin": "cd apps/admin && npm run dev",
    "dev:api": "cd apps/api && npm run dev",
    "dev:concurrent": "concurrently --kill-others-on-fail --names \"WEB,ADMIN,API\" -c \"cyan,green,yellow\" \"npm run dev:web\" \"npm run dev:admin\" \"npm run dev:api\"",
    "build": "turbo run build",
    "build:web": "cd apps/web && npm run build",
    "build:admin": "cd apps/admin && npm run build",
    "build:prod": "npm run fix && npm run build",
    "build:docker": "docker-compose build --parallel",
    "build:analyze": "ANALYZE=true next build",
    "fix": "npm run prisma:generate",
    "lint": "next lint",
    "test": "jest",
    "clean": "rimraf .next node_modules/.cache",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "fix:install": "npm install --legacy-peer-deps",
    "fix:build": "npm run prisma:generate && npm run build",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset",
    "test:e2e": "playwright test",
    "test:e2e:all": "node scripts/run-e2e-tests.js",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report test-reports/html",
    "test:unit": "jest --testPathPattern=unit",
    "test:coverage": "jest --coverage",
    "test:e2e:ui": "playwright test --ui",
    "test:bot": "node scripts/run-e2e-tests.js",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md}\"",
    "type-check": "tsc --noEmit",
    "build:test": "NODE_ENV=test next build",
    "clean:cache": "rm -rf .next/cache",
    "clean:all": "rimraf .next node_modules package-lock.json",
    "prepare": "husky install",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop sooq-mazad-app",
    "pm2:restart": "pm2 restart sooq-mazad-app",
    "pm2:logs": "pm2 logs sooq-mazad-app",
    "pm2:status": "pm2 status",
    "health:check": "node scripts/healthcheck.js",
    "quality:full": "npm run type-check && npm run lint",
    "security:audit": "npm audit --audit-level high",
    "fix:permissions": "chmod +x .husky/pre-commit && chmod +x .husky/commit-msg && chmod +x .husky/pre-push",
    "test:build": "npm run build && echo 'Build test completed successfully'",
    "deploy:check": "npm run quality:full && npm run test:build && echo 'Ready for deployment'",
    "postinstall": "prisma generate",
    "reinstall": "npm run clean:all && npm install",
    "check:types": "tsc --noEmit",
    "check:lint": "next lint",
    "check:all": "npm run check:types && npm run check:lint",
    "perf:db": "node scripts/optimize-database.js",
    "perf:monitor": "node scripts/monitor-performance.js",
    "perf:optimize": "node scripts/optimize-performance.js",
    "perf:images": "node scripts/optimize-images.js",
    "perf:compress": "node scripts/compress-images.js",
    "cache:clear": "node scripts/clear-cache.js",
    "build:optimized": "next build",
    "start:no-redis": "set KEYDB_ENABLED=false && npm run dev",
    "fix:all": "npm run prisma:generate && npm run lint:fix",
    "fix:prisma": "npx prisma generate",
    "build:force": "rmdir /s /q .next 2>nul & next build --no-lint",
    "start": "next start -p 3021",
    "start:prod": "NODE_ENV=production next start -p 3021",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:seed": "node prisma/seed.js",
    "setup": "npm install && npm run db:generate && npm run build",
    "reset": "npm run clean:all && npm install && npm run setup",
    "perf:all": "npm run perf:optimize && npm run perf:images && npm run perf:compress",
    "a11y:check": "echo \"Accessibility check - see ENTERPRISE_ANALYSIS_REPORT.json\""
  },
